echo SERVICE..
kubectl --kubeconfig=/root/.kube/k8s-yl-kubeconfig.yaml describe service nginx-service
echo ----------------------------
echo INDRESS..
kubectl --kubeconfig=/root/.kube/k8s-yl-kubeconfig.yaml describe ingress my-ingres
echo ----------------------------
echo DEPLOYMENT..
kubectl --kubeconfig=/root/.kube/k8s-yl-kubeconfig.yaml describe deployment nginx-fastapi-deployment